@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<body>
    <div class="content-wrap">
        <div class="content-left">
            <div class="main-wrap">
                <div id="title-Day"></div>
                <div>/</div>
                <div id="titlet-Month"></div>
                <div>/</div>
                <div id="title-Year"></div>
            </div>
            <div class="todo-wrap">
                <div class="todo-title">Todo List </div>
                <div class="todoList" id="todoslist"> 
                    <div id="input-list" class="input-list"></div>
                    
                </div>
                <div class="diary-title">Diary List</div>
                <div class="diary_list" id="diarylist">
                </div>
            </div>
        </div>
        <div class="content-right">
            <table id="calendar" align="center">
                <thead>
                    <tr class="btn-wrap clearfix">
                        <td>
                            <span id="prev">
                                &#60;
                                <span />
                        </td>
                        <td align="center" id="current-year" colspan="2"></td>
                        <td align="center" id="current-year-month" colspan="2"></td>
                        <td>
                            <span id="next">
                                &#62;
                            </span>
                        </td>
                    </tr>
                    <tr>
                        <td class="sun" align="center">Sun</td>
                        <td align="center">Mon</td>
                        <td align="center">Tue</td>
                        <td align="center">Wed</td>
                        <td align="center">Thu</td>
                        <td align="center">Fri</td>
                        <td class="sat" align="center">Sat</td>
                    </tr>
                </thead>
                <tbody id="calendar-body" class="calendar-body"></tbody>
            </table>
        </div>
    </div>
</body>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/calendarjs")
@Styles.Render("~/Content/calendarcss")


